package bean;

public class Modelo {
	
	private String modelo;

	public Modelo(String modelo) {
		super();
		this.modelo = modelo;
	}

	public Modelo() {
		super();
	}

	public String getModelo() {
		return modelo;
	}

	public void setModelo(String modelo) {
		this.modelo = modelo;
	}

	@Override
	public String toString() {
		return "Modelo [modelo=" + modelo + "]";
	}
	
	

}
