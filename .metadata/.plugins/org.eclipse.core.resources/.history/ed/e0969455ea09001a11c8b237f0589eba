package bean;

public class Marca {
	
	private String marca;

	public Marca(String marca) {
		super();
		this.marca = marca;
	}

	public Marca() {
		super();
	}

	public String getMarca() {
		return marca;
	}

	public void setMarca(String marca) {
		this.marca = marca;
	}

	@Override
	public String toString() {
		return "Marca [marca=" + marca + "]";
	}
	
	

}
